super + Return
	exec $TERM
super + s
	scpad
super + w
	~/src/zen/zen
super + shift + E
	~/.local/scripts/exit.sh
super + t
	telegram-desktop
super + r
	$TERM -e ranger
super + shift + R
	thunar
super + a
	mypaint
super + {equal, minus, BackSpace}
	~/.local/scripts/vol.sh {up, down, toggle}
super + c
	screenshot
super + shift + C
	screenshot select
super + control + c
	screenshot window
super + shift + P
	mpv $(xclip -o)
super + m
	dmount
super + control + m
	dumount
super + d
	~/src/Discord/Discord
